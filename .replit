run = "bash setup_ffmpeg.sh && python main.py"
entrypoint = "main.py"
modules = ["python-3.10"]

[languages]

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = "pylsp"

[deployment]
run = ["bash", "setup_ffmpeg.sh", "python3", "main.py"]
deploymentTarget = "cloudrun"
