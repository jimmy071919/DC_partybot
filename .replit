run = "sudo apt-get update && sudo apt-get install -y ffmpeg && python main.py"
entrypoint = "main.py"
modules = ["python-3.10"]

[languages]

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = "pylsp"

[deployment]
run = ["sudo apt-get update", "sudo apt-get install -y ffmpeg", "python3", "main.py"]
deploymentTarget = "cloudrun"
